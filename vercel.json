{
  "version": 2,
  "buildCommand": "vite build --config vite.config.vercel.ts",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": "vite",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}